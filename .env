#Rules Configuration
RULES_DIR="rules/"

#Each Target Event **MUST** contains CASE_ID_FIELD column!
#For Process Mining, Event Log must contain Case ID in order to distinguish Process Instance
#CASE_ID_FIELD is important for the tool to place events into corresponding Process Instance Files
CASE_ID_FIELD="processInstanceId"

#Each Target Event **MUST** contains EVENT_DATE column!
EVENT_DATE="eventTime"

OUTPUT_DIR="output/"
SUBFILE_OUTPUT_DIR="output/subfiles/"
EXP_OUTPUT_DIR="exp_output/"

#Configure sorting files by MINUTE, HALFHOUR, HOUR, DAY
SORTING_CONFIG="MINUTE"

#Source MySQL Database
SOURCE_HOST="localhost"
SOURCE_USER="jbpm"
SOURCE_PASSWORD="jbpm"
SOURCE_DATABASE="36000_tasks"

